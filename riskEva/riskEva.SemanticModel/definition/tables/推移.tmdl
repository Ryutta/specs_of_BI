	measure trend_all =
			
			VAR
			_jisseki18to24 =
			CALCULATE(
			    SUM('購買明細'[実績明細金額])/1000000,
			    FILTER('Calendar',VALUE('Calendar'[FY])<2025),
			    'Dim_区分'[区分]="修繕費",
			    not '購買明細'[13分類rev] = "SZ"
			)
			// ※20251111_リスク評価ベースからMoneMに変更
			// VAR
			// _2025yosan =
			// CALCULATE(
			//     SUM('Query_2025'[申請予算])/1000,
			//     TREATAS(VALUES('Calendar'[FY]),'Query_2025'[年度])
			// )
			VAR
			_2026yosan =
			CALCULATE(
			    [申請額],
			    TREATAS(VALUES('Calendar'[FY]),'riskEva2026'[年度])
			)
			VAR
			_other =
			CALCULATE(
			    SUM('期中取得/材修'[実績金額])/1000000,
			    TREATAS(VALUES('Calendar'[FY]),'期中取得/材修'[予算年度]),
			    TREATAS(VALUES('修繕費対応分類表'[周期性]),'期中取得/材修'[勘定名称2])
			)
			VAR
			_2025plan =
			CALCULATE(
			    SUM('起業費・修繕費'[計画予算金額])/1000000,
			    TREATAS(VALUES('Calendar'[Date]),'起業費・修繕費'[購買依頼日2]),
			    '起業費・修繕費'[予算年度]="2025"
			)
			
			RETURN
			_jisseki18to24+_2026yosan+_other+_2025plan+0
		formatString: #,0
		lineageTag: 09244bd1-393a-40e2-88ae-4c5b137afc4a

